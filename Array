    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int ary[] = new int[n+1];
        int arr[] = new int[0];
        for (int i = 0;i<=n;i++){
            ary[i] = sc.nextInt();
            ary[i] = arr[i];
            System.out.print(ary[i]);
        }

        for (int  i = 0;i<=n;i++){
            ary[i] = ary.length-1;
        }
    }
