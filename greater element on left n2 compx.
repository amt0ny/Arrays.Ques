    public static void main(String[] args) {
     int arr[] = {1,3,0,0,0,0,5,2,4};
     for (int i = 0;i<arr.length;i++){
         for (int j = i;j>=0;j--){
             if (arr[j] > arr[i]){
                 System.out.println(arr[j]);
                 break;
             }
             else if (arr[j] <= arr[i] && j == 0){
                 System.out.println(-1);
             }
         }
     }
    }
