    public static void main(String[] args) {
        int arr[] = {2,4,6,12,8,9,5,3,1};
        int max =arr[arr.length-1];
        System.out.println(max);
//        System.out.println(max);
        for (int i = arr.length-1;i>=0;i--){

            if (max < arr[i]){
                max = arr[i];
                System.out.println(max);
            }
        }
    }
