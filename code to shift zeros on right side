    public static void main(String[] args) {
        int arr[] = {1,2,9,0,4,2,1,5,6,0,0,0,0,0};
        int j =0;
        for (int i = 0;i<arr.length;i++){
            if (arr[i] != 0){
                arr[j] = arr[i];
                j++;
            }
        }
        while (j<arr.length-1){
            arr[j] = 0;
            j++;
        }
        for (int i = 0;i<arr.length;i++){
            System.out.print(arr[i] + " ");
        }
    }
