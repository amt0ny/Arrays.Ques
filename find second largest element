    public static void main(String[] args) {
        int arr[] = {10,6,1,9,4,7,8};
        int max = 0;
        int secmax = 0;
        for (int i = 0;i<arr.length;i++){
            if (max < arr[i]){
                secmax = max;
                max = arr[i];
            }
            else if (secmax < arr[i]){
                secmax = arr[i];
            }
        }
        System.out.println(max);
        System.out.println(secmax);

    }
