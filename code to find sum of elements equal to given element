    public static void main(String[] args) {
        int arr[] = {2,5,3,7,9,1};
        int num = 8;
        for (int i= 0;i<arr.length;i++){
            for ( int j = 0 ;j<arr.length;j++){
                if (arr[i] + arr[j] == num && i != j){
                    System.out.println(arr[i] + " " + arr[j]);
                }
            }
        }
    }
