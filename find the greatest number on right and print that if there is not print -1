     for (int i = 0;i<arr.length;i++){
         for(int j = i+1;j<arr.length;j++){
             if (arr[j] > arr[i]){
                 System.out.println(arr[j]);
                 break;
             }
             if (arr[j] < arr[i] && j == arr.length-1){
                 System.out.println(-1);
             }

         }if ( i == arr.length-1){
             System.out.println(-1);
         }

     }
