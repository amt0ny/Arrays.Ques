        int ary[] = {1,2,3,4,6,7,8};
        int sum = 0;
        int sum2 = 0;
        int n = ary[ary.length-1];
        int l = ary.length;
        System.out.println(n);
        for (int i=0;i<=l-1;i++){
            sum += ary[i];
        }
        for (int i =0;i<=n;i++){
            sum2 += i;
        }
        System.out.println(sum2-sum);
    }
}
