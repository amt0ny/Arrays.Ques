    public static void main(String[] args) {
        int arr[] = {100,80,60,70,60,75,85};
        int count = 0;
        for (int i = 0;i<arr.length;i++){
            for (int j = i;j>=0;j--){
                if (arr[i] >= arr[j]){
                    count++;
                }
                else {
                    break;
                }
            }
            System.out.println(count);
            count = 0;
        }
    }
